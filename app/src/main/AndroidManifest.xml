<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.layrif.infowargatrue">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Makanan" />
        <activity android:name=".Minuman" />
        <activity android:name=".Baju" />
        <activity android:name=".Bangunan" />
        <activity android:name=".Elektronik" />
        <activity android:name=".DetailMakanan"></activity>
        <activity android:name=".DetailMinuman"></activity>
        <activity android:name=".DetailBaju"></activity>
        <activity android:name=".DetailBangunan"></activity>
        <activity android:name=".DetailElektronik"></activity>
    </application>

</manifest>